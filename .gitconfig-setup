# This is Git's per-user configuration file.

# Terminal settings for kitteh mode:
[color]
  branch = auto
  diff = auto
  status = auto

[color "branch"]
  current = magenta
  local = yellow
  remote = yellow

[color "diff"]
  meta = yellow bold
  frag = magenta bold
  old = red bold
  new = cyan bold

[color "status"]
  added = yellow bold
  changed = magenta bold
  untracked = cyan bold

[url "git@github.com:"]
	insteadOf = https://github.com/
